{
  "name": "newrank-design",
  "version": "1.0.0",
  "description": "",
  "typings": "lib/index.d.ts",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:types && gulp",
